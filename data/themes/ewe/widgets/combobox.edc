group { name: "ewe/combobox/base/default";
   images {
      image: "dropdown-bg.png" COMP;
      image: "dropdown-focus-bg.png" COMP;
      image: "dropdown-bg_unactive.png" COMP;
      image: "icon-slidedown.png" COMP;
      image: "icon-slideup.png" COMP;
   }
   min: 80 19;
   max: 9999 19;
   parts {
      part { name: "base";
         type: SPACER;
         description { state: "default" 0.0;
            max: -1 19;
         }
      }
      part { name: "bg";
         type: IMAGE;
         description { state: "default" 0.0;
            rel1 {
               to: "base";
            }
            rel2 {
               to: "base";
            }
            image {
               normal: "dropdown-bg.png";
               border: 3 3 1 1;
               middle: DEFAULT;
            }
         }
         description { state: "focus" 0.0;
            rel1 {
               to: "base";
            }
            rel2 {
               to: "base";
            }
            image {
               normal: "dropdown-focus-bg.png";
               border: 3 3 1 1;
               middle: DEFAULT;
            }
         }
         description { state: "disable" 0.0;
            rel1 {
               to: "base";
            }
            rel2 {
               to: "base";
            }
            image {
               normal: "dropdown-bg_unactive.png";
               border: 3 3 1 1;
               middle: DEFAULT;
            }
         }
      }
      part { name: "left.offset";
         type: SPACER;
         description { state: "default" 0.0;
            align: 0.0 0.0;
            min: 10 0;
            max: 10 -1;
            rel1 {
               to: "base";
            }
            rel2 {
               to: "base";
            }
         }
      }
      part { name: "right.offset";
         type: SPACER;
         description { state: "default" 0.0;
            align: 1.0 1.0;
            min: 20 19;
            max: 20 -1;
            rel2 {
               to: "bg";
            }
         }
      }
      part { name: "arrow";
         type: IMAGE;
         clip_to: "disabler";
         description { state: "default" 0.0;
            max: 6 4;
            rel1 {
               to: "right.offset";
            }
            rel2 {
               to: "right.offset";
            }
            image {
               normal: "icon-slidedown.png";
               middle: DEFAULT;
            }
         }
         description { state: "expanded" 0.0;
            max: 6 4;
            rel1 {
               to: "right.offset";
            }
            rel2 {
               to: "right.offset";
            }
            image {
               normal: "icon-slideup.png";
               middle: DEFAULT;
            }
         }
      }
      part { name: "ewe.text";
         type: TEXT;
         clip_to: "disabler";
         description { state: "default" 0.0;
            align: 0.0 0.5;
            color: 213 213 213 255;
            rel1 {
               relative: 1.0 0.0;
               to_x: "left.offset";
               to_y: "base";
            }
            rel2 {
               relative: 0.0 1.0;
               to_x: "right.offset";
               to_y: "base";
            }
            text {
               font: FN;
               size: 12;
               max: 1 1;
            }
         }
      }
      part { name: "expand.btn";
         type: RECT;
         description { state: "default" 0.0;
            color: 0 0 0 0;
            rel1 {
               to: "bg";
            }
            rel2 {
               to: "bg";
            }
         }
         description { state: "unvisible" 0.0;
            visible: 0;
            color: 0 0 0 0;
            rel1 {
               to: "bg";
            }
            rel2 {
               to: "bg";
            }
         }
      }
      part { name: "disabler";
         type: RECT;
         description { state: "default" 0.0;
            rel1 {
               to: "bg";
            }
            rel2 {
               to: "bg";
            }
         }
         description { state: "active" 0.0;
            color: 64 64 64 225;
            rel1 {
               to: "bg";
            }
            rel2 {
               to: "bg";
            }
         }
      }
   }
   programs {
      program { name: "expand";
         signal: "ewe,state,expanded";
         source: "ewe";
         action: STATE_SET "expanded" 0.00;
         target: "arrow";
      }
      program { name: "focus";
         signal: "ewe,state,expanded";
         source: "ewe";
         action: STATE_SET "focus" 0.00;
         target: "bg";
      }
      program { name: "collapse";
         signal: "ewe,state,default";
         source: "ewe";
         action: STATE_SET "default" 0.00;
         target: "arrow";
      }
      program { name: "outfocus";
         signal: "ewe,state,default";
         source: "ewe";
         action: STATE_SET "default" 0.00;
         target: "bg";
      }
      program { name: "clicked";
         signal: "mouse,clicked,1";
         source: "expand.btn";
         action: SIGNAL_EMIT "clicked" "ewe";
      }
      program { name: "btn_show";
         signal: "btn,show";
         source: "ewe";
         action: STATE_SET "default" 0.00;
         target: "expand.btn";
      }
      program { name: "btn_hide";
         signal: "btn,hide";
         source: "ewe";
         action: STATE_SET "unvisible" 0.00;
         target: "expand.btn";
      }
      program { name: "back_disable";
         signal: "ewe,state,disable";
         source: "ewe";
         action: STATE_SET "disable" 0.00;
         target: "bg";
      }
      program { name: "disabler_active";
         signal: "ewe,disable,active";
         source: "ewe";
         action: STATE_SET "active" 0.00;
         target: "disabler";
      }
      program { name: "disabler_unactive";
         signal: "ewe,disable,unactive";
         source: "ewe";
         action: STATE_SET "default" 0.00;
         target: "disabler";
      }
   }
}

