group { name: "ewe/combobox/base/small";
   min: 0 21;
   max: 21 21;
   images {
      image: "icon-add.png" COMP;
      image: "icon-slideup.png" COMP;
      image: "icon-slidedown.png" COMP;
      image: "combobox_small_bg_unactive.png" COMP;
      image: "combobox_small_bg.png" COMP;
      image: "combobox_small_bg_focused.png" COMP;
   }
   parts {
      part { name: "bg"; type: RECT;
         description {
            state: "default" 0;
            color: TRANSPARENT_COLOR;
         }
      }
      part { name: "back";
         type: IMAGE;
         description { state: "default" 0.0;
            align: 0.0 0.0;
            fixed: 1 1;
            image {
               normal: "combobox_small_bg.png";
               border:  4 4 9 9;
            }
            min: 31 21;
            rel1 {
               to: "bg";
               relative: 0.0 0.0;
            }
            rel2.to: "bg";
         }
         description { state: "focus" 0.0;
            inherit: "default" 0.0;
            image.normal: "combobox_small_bg_focused.png";
         }
         description { state: "disable" 0.0;
            inherit: "default" 0.0;
            image.normal: "combobox_small_bg_unactive.png";
         }
      }
      part { name: "left.offset";
         type: SPACER;
         description { state: "default";
            align: 0.0 0.5;
            min: 25 21;
            max: 25 21;
            rel1.to: "back";
         }
      }
      part { name: "right.offset";
         type: SPACER;
         description { state: "default";
            align: 1.0 0.5;
            min: 10 21;
            max: 10 21;
            rel2.to: "back";
         }
      }
      part { name: "arrow";
         type: IMAGE;
         mouse_events: 1;
         clip_to: "disabler";
         description { state: "default" 0.0;
            align: 0.0 0.5;
            max: 6 4;
            image.normal: "icon-slidedown.png";
            rel1.to: "right.offset";
            rel2.to: "right.offset";
         }
         description { state: "expanded" 0.0;
            inherit: "default" 0.0;
            image.normal: "icon-slideup.png";
         }
      }
      part { name: "plus";
         type: IMAGE;
         mouse_events: 1;
         clip_to: "disabler";
         description { state: "default" 0.0;
            align: 0.5 0.5;
            max: 16 16;
            image.normal: "icon-add.png";
            rel1.to: "left.offset";
            rel2.to: "left.offset";
         }
      }

      part { name: "ewe.text";
         type: TEXT;
         clip_to: "disabler";
         description { state: "default" 0.0;
            fixed: 1 1;
            align: 0.5 0.5;
            visible: 0;
            color: COMBOBOX_TEXT_COLOR;
            text {
               font: FN;
               min: 1 0;
               elipsis: -1;
               size: 12;
               align: 0.5 0.5;
            }
            rel1 {
               to_x: "left.offset";
               relative: 1.0 0.0;
            }
            rel2 {
               to_x: "right.offset";
               relative: 0.0 1.0;
               offset: -3 -1;
            }
         }
      }
      part { name: "expand.btn";
         type: RECT;
         description { state: "default" 0.0;
            visible: 1;
            color: TRANSPARENT_COLOR;
            rel1.to: "back";
            rel2.to: "back";
         }
         description { state: "unvisible" 0.0;
            inherit: "default" 0.0;
            visible: 0;
         }
      }
      part { name: "disabler";
         type: RECT;
         description { state: "default" 0.0;
            color: WHITE_COLOR;
            rel1.to: "back";
            rel2.to: "back";
         }
         description { state: "active" 0.0;
            inherit: "default" 0.0;
            color: 64 64 64 225;
         }
      }
   }
   programs {
      program { name: "expand";
         signal: "ewe,state,expanded";
         source: "ewe";
         action: STATE_SET "expanded" 0.0;
         target: "arrow";
      }
      program { name: "focus";
         signal: "ewe,state,expanded";
         source: "ewe";
         action: STATE_SET "focus" 0.0;
         target: "back";
      }
      program { name: "collapse";
         signal: "ewe,state,default";
         source: "ewe";
         action: STATE_SET "default" 0.0;
         target: "arrow";
      }
      program { name: "outfocus";
         signal: "ewe,state,default";
         source: "ewe";
         action: STATE_SET "default" 0.0;
         target: "back";
      }
      program { name: "clicked";
         signal: "mouse,clicked,1";
         source: "expand.btn";
         action: SIGNAL_EMIT "clicked" "ewe";
      }
      program { name: "btn_show";
         signal: "btn,show";
         source: "ewe";
         action: STATE_SET "default" 0.0;
         target: "expand.btn";
      }
      program { name: "btn_hide";
         signal: "btn,hide";
         source: "ewe";
         action: STATE_SET "unvisible" 0.0;
         target: "expand.btn";
      }
      program { name: "back_disable";
